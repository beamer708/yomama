    "@types/react": "^18.3.0",
    "@types/react-dom": "^18.3.0",
    "autoprefixer": "^10.4.17",
    "eslint": "^9.39.2",
    "eslint-config-next": "^16.1.6",
    "postcss": "^8.4.33",
    "prisma": "^6.19.2",
    "tailwindcss": "^3.4.1",
    "tsx": "^4.19.0",
    "typescript": "^5.3.3"
